{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": [
		"react"
	],
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true,
		"jasmine": true
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
    	"indent": [
			"error",
			"tab",
			{"SwitchCase": 1}
		],
		"semi": ["error", "always"],
		"no-console": "off",
		"no-unused-vars": "warn"
  	}
}